{
    "name": "eduardomaciel/test-sdk",
    "autoload": {
        "psr-4": {
            "Eduardomaciel\\TestSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Eduardo Maciel",
            "email": "mrgamex49@gmail.com"
        }
    ],
    "require": {
        "open-pix/php-sdk": "dev-main",
        "php": "^7.4 || ^8.0",
        "php-http/discovery": "^1.18",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
